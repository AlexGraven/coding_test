<p id="notice"><%= notice %></p>
<h1>Update <%= @update.id %></h1>
<h2>Uploaded <%= @update.updated_at %></h2>

<h2>Users:</h2>
<div>
<table>
<tr>
  <th>First Name</th>
  <th>Last Name</th>
  <th>Age</th>
  <th>Sex</th>
  <th>Personality</th>
</tr>
<% @update.users.each do |user|%>
  <tr>
    <td><%=user.first_name%></td>
    <td><%=user.last_name%></td>
    <td><%=user.age%></td>
    <td><%=user.sex%></td>
    <td><%=user.personality%></td>
  </tr>
<% end %>
</table>
</div>
<div>
<%= link_to 'Back', updates_path %>
</div>
